  fhir:concept ( [
     fhir:code [ fhir:v "access" ] ;
     fhir:display [ fhir:v "Access/View Record Lifecycle Event" ] ;
     fhir:definition [ fhir:v "Occurs when an agent causes the system to obtain and open a record entry for inspection or review." ]
  ] [
     fhir:code [ fhir:v "hold" ] ;
     fhir:display [ fhir:v "Add Legal Hold Record Lifecycle Event" ] ;
     fhir:definition [ fhir:v "Occurs when an agent causes the system to tag or otherwise indicate special access management and suspension of record entry deletion/destruction, if deemed relevant to a lawsuit or which are reasonably anticipated to be relevant or to fulfill organizational policy under the legal doctrine of “duty to preserve”." ]
  ] [
     fhir:code [ fhir:v "amend" ] ;
     fhir:display [ fhir:v "Amend (Update) Record Lifecycle Event" ] ;
     fhir:definition [ fhir:v "Occurs when an agent makes any change to record entry content currently residing in storage considered permanent (persistent)." ]
  ] [
     fhir:code [ fhir:v "archive" ] ;
     fhir:display [ fhir:v "Archive Record Lifecycle Event" ] ;
     fhir:definition [ fhir:v "Occurs when an agent causes the system to create and move archive artifacts containing record entry content, typically to long-term offline storage." ]
  ] [
     fhir:code [ fhir:v "attest" ] ;
     fhir:display [ fhir:v "Attest Record Lifecycle Event" ] ;
     fhir:definition [ fhir:v "Occurs when an agent causes the system to capture the agent’s digital signature (or equivalent indication) during formal validation of record entry content." ]
  ] [
     fhir:code [ fhir:v "decrypt" ] ;
     fhir:display [ fhir:v "Decrypt Record Lifecycle Event" ] ;
     fhir:definition [ fhir:v "Occurs when an agent causes the system to decode record entry content from a cipher." ]
  ] [
     fhir:code [ fhir:v "deidentify" ] ;
     fhir:display [ fhir:v "De-Identify (Anononymize) Record Lifecycle Event" ] ;
     fhir:definition [ fhir:v "Occurs when an agent causes the system to scrub record entry content to reduce the association between a set of identifying data and the data subject in a way that might or might not be reversible." ]
  ] [
     fhir:code [ fhir:v "deprecate" ] ;
     fhir:display [ fhir:v "Deprecate Record Lifecycle Event" ] ;
     fhir:definition [ fhir:v "Occurs when an agent causes the system to tag record entry(ies) as obsolete, erroneous or untrustworthy, to warn against its future use." ]
  ] [
     fhir:code [ fhir:v "destroy" ] ;
     fhir:display [ fhir:v "Destroy/Delete Record Lifecycle Event" ] ;
     fhir:definition [ fhir:v "Occurs when an agent causes the system to permanently erase record entry content from the system." ]
  ] [
     fhir:code [ fhir:v "disclose" ] ;
     fhir:display [ fhir:v "Disclose Record Lifecycle Event" ] ;
     fhir:definition [ fhir:v "Occurs when an agent causes the system to release, transfer, provision access to, or otherwise divulge record entry content." ]
  ] [
     fhir:code [ fhir:v "encrypt" ] ;
     fhir:display [ fhir:v "Encrypt Record Lifecycle Event" ] ;
     fhir:definition [ fhir:v "Occurs when an agent causes the system to encode record entry content in a cipher." ]
  ] [
     fhir:code [ fhir:v "extract" ] ;
     fhir:display [ fhir:v "Extract Record Lifecycle Event" ] ;
     fhir:definition [ fhir:v "Occurs when an agent causes the system to selectively pull out a subset of record entry content, based on explicit criteria." ]
  ] [
     fhir:code [ fhir:v "link" ] ;
     fhir:display [ fhir:v "Link Record Lifecycle Event" ] ;
     fhir:definition [ fhir:v "Occurs when an agent causes the system to connect related record entries." ]
  ] [
     fhir:code [ fhir:v "merge" ] ;
     fhir:display [ fhir:v "Merge Record Lifecycle Event" ] ;
     fhir:definition [ fhir:v "Occurs when an agent causes the system to combine or join content from two or more record entries, resulting in a single logical record entry." ]
  ] [
     fhir:code [ fhir:v "originate" ] ;
     fhir:display [ fhir:v "Originate/Retain Record Lifecycle Event" ] ;
     fhir:definition [ fhir:v "Occurs when an agent causes the system to: a) initiate capture of potential record content, and b) incorporate that content into the storage considered a permanent part of the health record." ]
  ] [
     fhir:code [ fhir:v "pseudonymize" ] ;
     fhir:display [ fhir:v "Pseudonymize Record Lifecycle Event" ] ;
     fhir:definition [ fhir:v "Occurs when an agent causes the system to remove record entry content to reduce the association between a set of identifying data and the data subject in a way that may be reversible." ]
  ] [
     fhir:code [ fhir:v "reactivate" ] ;
     fhir:display [ fhir:v "Re-activate Record Lifecycle Event" ] ;
     fhir:definition [ fhir:v "Occurs when an agent causes the system to recreate or restore full status to record entries previously deleted or deprecated." ]
  ] [
     fhir:code [ fhir:v "receive" ] ;
     fhir:display [ fhir:v "Receive/Retain Record Lifecycle Event" ] ;
     fhir:definition [ fhir:v "Occurs when an agent causes the system to a) initiate capture of data content from elsewhere, and b) incorporate that content into the storage considered a permanent part of the health record." ]
  ] [
     fhir:code [ fhir:v "reidentify" ] ;
     fhir:display [ fhir:v "Re-identify Record Lifecycle Event" ] ;
     fhir:definition [ fhir:v "Occurs when an agent causes the system to restore information to data that allows identification of information source and/or information subject." ]
  ] [
     fhir:code [ fhir:v "unhold" ] ;
     fhir:display [ fhir:v "Remove Legal Hold Record Lifecycle Event" ] ;
     fhir:definition [ fhir:v "Occurs when an agent causes the system to remove a tag or other cues for special access management had required to fulfill organizational policy under the legal doctrine of “duty to preserve”." ]
  ] [
     fhir:code [ fhir:v "report" ] ;
     fhir:display [ fhir:v "Report (Output) Record Lifecycle Event" ] ;
     fhir:definition [ fhir:v "Occurs when an agent causes the system to produce and deliver record entry content in a particular form and manner." ]
  ] [
     fhir:code [ fhir:v "restore" ] ;
     fhir:display [ fhir:v "Restore Record Lifecycle Event" ] ;
     fhir:definition [ fhir:v "Occurs when an agent causes the system to recreate record entries and their content from a previous created archive artefact." ]
  ] [
     fhir:code [ fhir:v "transform" ] ;
     fhir:display [ fhir:v "Transform/Translate Record Lifecycle Event" ] ;
     fhir:definition [ fhir:v "Occurs when an agent causes the system to change the form, language or code system used to represent record entry content." ]
  ] [
     fhir:code [ fhir:v "transmit" ] ;
     fhir:display [ fhir:v "Transmit Record Lifecycle Event" ] ;
     fhir:definition [ fhir:v "Occurs when an agent causes the system to send record entry content from one (EHR/PHR/other) system to another." ]
  ] [
     fhir:code [ fhir:v "unlink" ] ;
     fhir:display [ fhir:v "Unlink Record Lifecycle Event" ] ;
     fhir:definition [ fhir:v "Occurs when an agent causes the system to disconnect two or more record entries previously connected, rendering them separate (disconnected) again." ]
  ] [
     fhir:code [ fhir:v "unmerge" ] ;
     fhir:display [ fhir:v "Unmerge Record Lifecycle Event" ] ;
     fhir:definition [ fhir:v "Occurs when an agent causes the system to reverse a previous record entry merge operation, rendering them separate again." ]
  ] [
     fhir:code [ fhir:v "verify" ] ;
     fhir:display [ fhir:v "Verify Record Lifecycle Event" ] ;
     fhir:definition [ fhir:v "Occurs when an agent causes the system to confirm compliance of data or data objects with regulations, requirements, specifications, or other imposed conditions based on organizational policy." ]
  ] ) . # 

